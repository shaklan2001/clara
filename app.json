{
  "expo": {
    "name": "Clara",
    "slug": "Clara",
    "scheme": "industry-ready-expo-app-setup",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F0F23"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shaklan.clara",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F0F23"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.shaklan.clara"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-asset",
      [
        "react-native-auth0",
        {
          "domain": "dev-gxp7532f5e60qup8.uk.auth0.com",
          "clientId": "gEssI9303S4zyeAEqezKJPdfocgGLJsw"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4f6b9366-e011-41a5-8d86-79b4df6e43d4"
      }
    }
  }
}
