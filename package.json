{
  "name": "clara",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "start:dev": "expo start --dev-client",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:dev:android": "eas build --profile development --platform android --local",
    "build:dev:ios": "eas build --profile development --platform ios --local",
    "build:preview:android": "eas build --profile preview --platform android",
    "build:preview:ios": "eas build --profile preview --platform ios",
    "build:preview:ios:local": "eas build --profile preview --platform ios --local",
    "build:prod:android": "eas build --profile production --platform android",
    "build:prod:ios": "eas build --profile production --platform ios",
    "build:prod:ios:local": "eas build --profile production --platform ios --local",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "submit:prod:android": "eas submit --platform android --profile production",
    "submit:prod:ios": "eas submit --platform ios --profile production",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "expo",
    "react-native",
    "expo-router",
    "typescript",
    "eslint",
    "prettier",
    "reanimated",
    "industry-ready",
    "production-ready",
    "modern-architecture"
